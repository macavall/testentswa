{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "globalHeaders": {
        "Cache-Control": "public, max-age=3600"
    },
    "mimeTypes": {
        ".html": "text/html",
        ".css": "text/css",
        ".js": "application/javascript"
    }
}